pipeline {
    agent { label 'java'}
    stages {
                    stage('Build hello.py'){ 
                        steps {
                            sh 'python hello.py'
                               }
                                               }
                    stage('Test hello.py'){ 
                        steps {
                            sh 'python hello.py'
                               }
                                             }
                    stage('Deploy hello.py'){ 
                        steps {
                            sh 'python hello.py'
                              }
                                               }
                   
                               }
                                                }
               
